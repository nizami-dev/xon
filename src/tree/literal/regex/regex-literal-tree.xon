import '../../../grammar/xon-parser': RegexLiteralContext
import '../../../lib/core': String
import '../../../util/source-range': SourceRange
import '../literal-tree': LiteralTree

object RegexLiteralTree(ctx RegexLiteralContext) LiteralTree
  ctx RegexLiteralContext = ctx
  sourceRange SourceRange = SourceRange.fromContext(ctx)
  value RegExp = #{new RegExp(this.pattern, 'g')}
  pattern String = ctx.text.slice(1, -1).replace(#{/\\\//g}, '/')
