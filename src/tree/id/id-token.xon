{none, None, String} := import '../lib/core'
{SourceRange} := import './source-range'
{Token} := import 'antlr4ts'

getIdToken := (token: Token?) => token and #{new IdToken(token)} or none

class IdToken(token: Token)
  sourceRange := SourceRange.fromToken(token)
  text := token.text
  toString := () => this.text
