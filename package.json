{
    "name": "@xon/ast",
    "author": "Nizami",
    "version": "1.2.7",
    "description": "XON is an open-source programming language",
    "bugs": {
        "url": "https://github.com/xon-lang/ast/issues"
    },
    "homepage": "https://github.com/xon-lang/ast#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xon-lang/ast.git"
    },
    "keywords": [
        "xon",
        "ast",
        "compiler",
        "translator",
        "programming language"
    ],
    "license": "MIT",
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "rimraf": "node_modules/rimraf/bin.js",
        "grammar": "scripts/grammar.sh",
        "test": "npm run grammar && jest --no-cache",
        "build": "scripts/build.sh",
        "pre-commit": "scripts/pre-commit.sh",
        "preversion": "npm run test",
        "version": "npm run build",
        "postversion": "git push && cd dist && npm publish --access public && rimraf ../dist"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run pre-commit"
        }
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "@types/node": "^14.14.21",
        "antlr4ts-cli": "^0.5.0-alpha.4",
        "create-ts-index": "^1.13.6",
        "husky": "^4.3.8",
        "jest": "^26.6.3",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.4.4",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "antlr4ts": "^0.5.0-alpha.4",
        "uuid": "^8.3.2"
    }
}
