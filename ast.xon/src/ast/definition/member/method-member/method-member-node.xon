MethodMemberNode(context) is MemberNode
  name = context.id().text
  isPrivate = name.startsWith('_')
  declaredGenerics = context.declaredGenerics()?.DEFINITION_ID().map(\x: x.text) || []
  parameters = context.parameters().toNodes()
  returnType = context.type().toNode()
  body = context.body().toNodes()
