LibraryNode is Node:
  context LibraryContext
  scope String
  name String
  members LibraryMemberNode[]

  $(context):
    $scope = context._scope.text
    $name = context._name.text
    $members = context.libraryMember().toNodes()

  toString() = "{$scope}.{$name}: {$members.join(', ')}"

LibraryContext.toNode = LibraryNode($)