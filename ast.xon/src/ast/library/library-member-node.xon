LibraryMemberNode is Node:
  context LibraryMemberContext
  name String
  alias String?

  $(context):
    $name = ctx._name.text
    $alias = ctx._alias?.text

  toString() = if $alias: "{$name} as {$alias}" else $name

LibraryMemberContext.toNode() = LibraryMemberNode($)

LibraryMemberContext[].toNodes() = $.map(\x: x.toNode())
