TypeNode is BaseNode
  name = $::type.name.replace("TypeNode", '')
  generics TypeNode[] = []

  markGenerics(generics String[]): $generics.forEach(\x: x.markGenerics(generics))

  useGenericsMap(genericsMap Map<String, TypeNode>) TypeNode

  getGenericsMap(type TypeNode) Map<String, TypeNode>

  infix ==(other: TypeNode) Boolean
