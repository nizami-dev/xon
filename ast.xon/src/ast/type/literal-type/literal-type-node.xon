LiteralTypeNode(context) extends TypeNode
  value = context.literal().toNode().value


  infix ==(other TypeNode) = other is LiteralTypeNode and $value === other.value

  useGenericsMap(genericsMap Map<string, TypeNode> = new Map()) = $

  getGenericsMap(type TypeNode) = Map<String, TypeNode>()

  toString() = if $value is String: `"{$value}"` else `{$value}`
